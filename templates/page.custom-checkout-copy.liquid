                store_key: store_key,
                discount_code: encodeURIComponent(discountCode),
                products_ids: products_ids, 
                variants_ids: variants_ids, 
                customer_id: customer_id,
                qtys: qties, 
                subtotal: cartJSON.items_subtotal_price/100
            };
